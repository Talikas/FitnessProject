<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.phrenologue.fitnessproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".SetUpProps"></activity>
        <activity android:name=".FirstProfile" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activityFeed.FeedView" />
        <activity android:name=".activityDailyResults.DailyResultsView" />
        <activity android:name=".activityInput.InputView" />
        <activity android:name=".activityProfile.ProfileView"/>
    </application>

</manifest>